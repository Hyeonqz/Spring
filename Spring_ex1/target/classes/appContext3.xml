<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans https://www.springframework.org/schema/beans/spring-beans-4.3.xsd">
	
	<!-- 사원데이터 설정 -->
	<bean id="sawon" class="Spring.di.ex3.SawonData">
	<constructor-arg value="현규"/>
	
	<property name="pay" value="5000000"></property>
	<property name="ipsaday" value="2023-10-04"></property>
	</bean>
	
	<!-- 사원 출력 -->
	<bean id="sawon1" class="Spring.di.ex3.Sawon">
		<constructor-arg ref="sawon"></constructor-arg>
	</bean>


</beans>
